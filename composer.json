{
	"name": "appfoster/upsnap",
	"description": "Monitoring & analytics plugin for Craft CMS sites.",
	"type": "craft-plugin",
	"keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "upsnap-plugin",
        "monitoring",
        "monitor",
        "watcher",
        "watch",
        "upsnap",
        "analytics",
        "upsnap plugin for craft"
    ],
    "support": {
        "docs": "https://github.com/Appfoster/upsnap/wiki",
        "issues": "https://github.com/Appfoster/upsnap/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Appfoster",
            "homepage": "https://www.appfoster.com/"
        }
    ],
	"require": {
		"craftcms/cms": "^5.0"
	},
	"autoload": {
		"psr-4": {
			"appfoster\\upsnap\\": "src/"
		}
	},
	"extra": {
		"name": "Upsnap",
        "handle": "upsnap",
        "hasCpSection": true,
		"changelogUrl": "https://github.com/Appfoster/upsnap/master/CHANGELOG.md",
        "documentationUrl": "https://github.com/Appfoster/upsnap/wiki",
		"class": "appfoster\\upsnap\\Upsnap"
	},
	"config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
